							"});",
							"pm.test(\"status code below verify\",function()",
							"{",
							"    pm.expect(pm.response.code).to.be.above(199);",
							"})",
							"pm.test(\"verify response time\",function()",
							"{",
							"    pm.expect(pm.response.responseTime).to.be.below(9000)",
							"})",
							"pm.expect(\"verify response time\",function()",
							"{",
							"    pm.expect(pm.response.responseTime).to.be.above(0);",
							"",
							"})",
							"pm.test(\"check Multiple headers\",function()",
							"{",
							"    data=[\"content-Length\",\"content-Type\",\"Date\"];",
							"    data.forEach( h=>",
							"    {",
							"        pm.response.to.have.header(h);",
							"",
							"    })",
							"});",
							"pm.test(\"Test status code using if-else\",function()",
							"{",
							"    let data=pm.response.code;",
							"    if(data===200)",
							"    {",
							"        pm.response.to.have.status(\"OK\");",
							"    }",
							"    else",
							"    {",
							"        pm.expect(data).to.eql(\"Not Found\");",
							"",
							"    }",
							"});",
							"pm.test(\"status code is not in oneof function \",function()",
							"{",
							"    pm.expect(pm.response.code).to.not.oneOf([201,202,203,204,205,206,207]);",
							"",
							"})",
							"pm.test(\"content-Length value check\",function()",
